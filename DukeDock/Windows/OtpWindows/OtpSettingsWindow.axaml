<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        Width="600"
        Height="600"
        x:Class="DukeDock.Windows.OtpWindows.OtpSettingsWindow"
        Title="OtpSettingsWindow">
    <Grid ColumnDefinitions="*,*" RowDefinitions="*,60">
        <ScrollViewer Grid.Row="0" Grid.Column="0">
            <ListBox>
                
            </ListBox>
        </ScrollViewer>
        <Button Grid.Row="1" Grid.Column="0" Content="Exit" Margin="10" HotKey="Escape" Command="{Binding ExitCommand}"></Button>
        <Button Grid.Row="1" Grid.Column="1" Content="Save" Margin="10" HotKey="Enter" Command="{Binding SaveCommand}"></Button>
    </Grid>
</Window>
